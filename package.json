  },
  "keywords": [
    "immutable",
    "redux"
  ],
  "author": {
    "name": "Gajus Kuizinas",
    "email": "gajus@anuary.com",
    "url": "http://gajus.com"
  },
  "license": "BSD-3-Clause",
